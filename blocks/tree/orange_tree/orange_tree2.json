{
	"format_version": "1.20.80",
	"minecraft:block": {
		"description": {
			"identifier": "pinatabedrock:orange_tree2",
			"menu_category": {
				"category": "nature",
				"group": "itemGroup.name.log"
			},
			"states": {
				"pinatabedrock:next_log": [
					0,
					1
				],
				"pinatabedrock:player_paced": [
					0,
					1
				],
				"pinatabedrock:fertilizer": [
					0,
					1,
					2,
					3
				]
			}
		},
		"components": {
			"tag:orange": {},
			"minecraft:geometry": {
				"identifier": "geometry.orange_tree2",
				"bone_visibility": {
					"bone": true,
					"sticks": "query.block_state('pinatabedrock:next_log') == 0 && query.block_state('pinatabedrock:player_paced') == 0"
				}
			},
			"minecraft:collision_box": {
				"origin": [
					-2,
					0,
					-2
				],
				"size": [
					4,
					16,
					4
				]
			},
			"minecraft:selection_box": {
				"origin": [
					-2,
					0,
					-2
				],
				"size": [
					4,
					16,
					4
				]
			},
			"minecraft:material_instances": {
				"*": {
					"texture": "pinatabedrock_orange_tree2",
					"render_method": "alpha_test"
				}
			},
			"minecraft:custom_components": [
				"pinatabedrock:random_tick"
			],
			"minecraft:destructible_by_explosion": {
				"explosion_resistance": 1
			},
			"minecraft:destructible_by_mining": {
				"seconds_to_destroy": 1
			},
			"minecraft:queued_ticking": {
				"interval_range": [
					10,
					10
				],
				"looping": true,
				"on_tick": {
					"event": "pinatabedrock:detect"
				}
			},
			"minecraft:on_player_placing": {
				"event": "pinatabedrock:player_placed"
			},
			"minecraft:on_interact": {
				"condition": "query.is_item_name_any('slot.weapon.mainhand', 0, 'pinatabedrock:fertilizer_orange')",
				"event": "pinatabedrock:fertilizer"
			}
		},
		"events": {
			"pinatabedrock:detect": {
				"sequence": [
					{
						"condition": "q.block_neighbor_has_any_tag(0,1,0,'orange')",
						"set_block_state": {
							"pinatabedrock:next_log": 1
						}
					},
					{
						"condition": "!q.block_neighbor_has_any_tag(0,1,0,'orange')",
						"set_block_state": {
							"pinatabedrock:next_log": 0
						}
					}
				]
			},
			"pinatabedrock:player_placed": {
				"set_block_state": {
					"pinatabedrock:player_paced": 1
				}
			},
			"pinatabedrock:fertilizer": {
				"decrement_stack": {
					"ignore_game_mode": false
				},
				"set_block_state": {
					"pinatabedrock:fertilizer": "q.block_state('pinatabedrock:fertilizer') + 1"
				}
			}
		}
	}
}