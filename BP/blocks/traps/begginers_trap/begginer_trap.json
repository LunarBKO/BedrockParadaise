{
	"format_version": "1.19.50",
	"minecraft:block": {
		"description": {
			"identifier": "pinatabedrock:begginer_trap"
		},
		"components": {
			"minecraft:geometry": "geometry.trap",
			"minecraft:material_instances": {
				"*": {
					"texture": "pinatabedrock_begginer_trap",
					"render_method": "alpha_test"
				}
			},
			"minecraft:light_emission": 0,
			"minecraft:destructible_by_mining": {
				"seconds_to_destroy": 2.0
			},
			"minecraft:destructible_by_explosion": {
				"explosion_resistance": 30
			},
			"minecraft:collision_box": {
				"origin": [
					-8,
					0,
					-8
				],
				"size": [
					16,
					2,
					16
				]
			},
			"minecraft:selection_box": {
				"origin": [
					-8,
					0,
					-8
				],
				"size": [
					16,
					2,
					16
				]
			},
			"minecraft:on_interact": {
				"event": "pinatabedrock:interact"
			},
			"minecraft:loot": "loot_tables/blocks/traps/begginer_trap/begginer_trap.json"
		},
		"events": {
			"pinatabedrock:interact": {
				"sequence": [
					{
						"condition": "query.is_item_name_any('slot.weapon.mainhand', 0, 'pinatabedrock:buttercup_petals')",
						"set_block": {
							"block_type": "pinatabedrock:begginer_trap_buttercup"
						},
						"decrement_stack": {
							"ignore_game_mode": false
						}
					},
					{
						"condition": "query.is_item_name_any('slot.weapon.mainhand', 0, 'pinatabedrock:buttercup_seed')",
						"set_block": {
							"block_type": "pinatabedrock:begginer_trap_buttercup_seed"
						},
						"decrement_stack": {
							"ignore_game_mode": false
						}
					},
					{
						"condition": "query.is_item_name_any('slot.weapon.mainhand', 0, 'minecraft:apple')",
						"set_block": {
							"block_type": "pinatabedrock:begginer_trap_apple"
						},
						"decrement_stack": {
							"ignore_game_mode": false
						}
					},
					{
						"condition": "query.is_item_name_any('slot.weapon.mainhand', 0, 'pinatabedrock:chili')",
						"set_block": {
							"block_type": "pinatabedrock:begginer_trap_chili"
						},
						"decrement_stack": {
							"ignore_game_mode": false
						}
					},
					{
						"condition": "query.is_sneaking",
						"set_block": {
							"block_type": "pinatabedrock:begginer_trap_closed"
						}
					}
				]
			}
		}
	}
}